{
  "name": "astro-digital-garden",
  "author": "stereobooster",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Starlight plugin for Astro digital garden.",
  "keywords": [
    "starlight",
    "plugin"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./braindb": {
      "import": "./dist/braindb.js",
